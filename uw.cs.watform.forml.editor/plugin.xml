<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<!--
-->

<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated Forml -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="uw.cs.watform.forml.forml.presentation.FormlModelWizardID"
            name="%_UI_FormlModelWizard_label"
            class="uw.cs.watform.forml.forml.presentation.FormlModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/FormlModelFile.gif">
         <description>%_UI_FormlModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated Forml -->
      <editor
            id="uw.cs.watform.forml.forml.presentation.FormlEditorID"
            name="%_UI_FormlEditor_label"
            icon="icons/full/obj16/FormlModelFile.gif"
            extensions="forml"
            class="uw.cs.watform.forml.forml.presentation.FormlEditor"
            contributorClass="uw.cs.watform.forml.forml.presentation.FormlActionBarContributor">
      </editor>
   </extension>

</plugin>
